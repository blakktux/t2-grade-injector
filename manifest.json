{
    "manifest_version": 2,
    "name": "T-Square Grade Injector",
    "version": "0.13",

    "permissions": [
        "*://t-square.gatech.edu/*"
    ],

    "content_scripts": [{
        "matches": [
            "*://t-square.gatech.edu/*"
        ],
        "js": ["content.js"]
    }],

    "background": {
        "scripts": ["background.js"]
    },

    "icons": { "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },

    "browser_action": {
        "default_title": "Choose a csv to inject",
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        }
    }
}